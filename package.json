{
  "name": "financisto",
  "version": "0.0.1",
  "private": false,
  "description": "A pet project, mainly for experiments and for analysing personal finances",
  "repository": {
    "git": "none"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --hot --inline",
    "start": "nodemon ./bin/www"
  },
  "license": "MIT",
  "author": "Ivan Demchenko <raqy.style@gmail.com>",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "mongodb": "^2.2.33",
    "morgan": "~1.9.0",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.8",
    "serve-favicon": "^2.4.5",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": "\\.test.js$"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "frontend/*",
      "bin/*",
      "dbdata/*",
      "public/*"
    ],
    "delay": "1500",
    "watch": [
      "service/",
      "routes",
      "views/",
      "app.js",
      "package.json"
    ],
    "env": {
      "DEBUG": "financier",
      "NODE_ENV": "development"
    }
  },
  "devDependencies": {
    "fantasy-readers": "0.0.1",
    "mongoose": "^4.12.3",
    "ramda": "^0.25.0",
    "supertest": "^3.0.0"
  }
}
